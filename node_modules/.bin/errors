#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules/@solana/errors/bin/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules/@solana/errors/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules/@solana/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/node_modules:/Users/shashank/Developer/dynamic agent/x402/node_modules:/Users/shashank/Developer/dynamic agent/node_modules:/Users/shashank/Developer/node_modules:/Users/shashank/node_modules:/Users/node_modules:/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules/@solana/errors/bin/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules/@solana/errors/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules/@solana/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/dynamic_agent/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/node_modules:/Users/shashank/Developer/dynamic agent/x402/node_modules:/Users/shashank/Developer/dynamic agent/node_modules:/Users/shashank/Developer/node_modules:/Users/shashank/node_modules:/Users/node_modules:/node_modules:/Users/shashank/Developer/dynamic agent/x402/examples/typescript/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@solana/errors/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../@solana/errors/bin/cli.mjs" "$@"
fi
